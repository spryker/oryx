{
  "packages": [
    "base/di"
  ],
  "version": "1.0.0-rc.1",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "forcePublish": true,
      "contents": "dist",
      "ignoreChanges": ["*.md", "**/*.spec.ts", "**/*.stories.ts"],
      "message": "chore(release): publish libs release [skip ci]",
      "tagVersionPrefix": "lib@",
      "verifyAccess": false,
      "exact": true
    }
  }
}
