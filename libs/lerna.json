    "template/application",
    "platform/push-notification",
    "platform/auth",
    "domain/cart",
    "domain/checkout",
    "domain/content",
    "platform/core",
    "platform/experience",
    "base/form",
    "platform/i18n",
    "base/di",
    "template/presets",
    "template/resources",
    "domain/product",
    "domain/picking",
    "domain/order",
    "platform/router",
    "platform/indexed-db",
    "platform/offline",
    "domain/search",
    "domain/site",
    "template/themes",
    "base/ui",
    "domain/user",
    "base/utilities",
    "template/cli"
  ],
  "version": "0.10.0",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "forcePublish": true,
      "contents": "dist",
      "ignoreChanges": ["*.md", "**/*.spec.ts", "**/*.stories.ts"],
      "message": "chore(release): publish libs release [skip ci]",
      "tagVersionPrefix": "lib@",
      "verifyAccess": false
    }
  }
}
